<div class="container">
    <h1 id="header">Eat-Da-Burger</h1>
    <img src="data:image/jpeg;base64,
    iIAiIgCIiAIiIAiIgCIiAIiIAiIgCIiAIiIAiIgCIiAIiID/9k=" alt="" id="center">

    <div class="row">
        <div class="col-6">
            <h2 class="t1">EAT THESE BURGERS</h2>
            {{#each burgers}}
            {{#if devoured}}
            <p>
                {{id}} {{burger_name}}
                <button data-burgerID="{{id}}" class="devour">Devour It!</button>
            </p>
            {{/if}}
            {{/each}}
        </div>
        <div class="col-6">
            <h2 class="t1">YUMMM DELICIOUS</h2>
            {{#each burgers}}
            {{#unless devoured}}
            <p>
                {{id}} {{burger_name}}
            </p>
            {{/unless}}
            {{/each}}
        </div>
    </div>
    <form action="">
        <div class="form-group" id="formStyle">
            <textarea class="form-control" id="burgerForm" rows="2" style="width: 400px"></textarea>
            <button class="btn btn-primary" type="submit" id="submitBurger">Submit Burger!</button>
        </div>
    </form>
</div>